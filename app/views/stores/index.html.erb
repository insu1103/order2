

<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>S name</th>
      <th>S num</th>
      <th>S add</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.s_name %></td>
        <td><%= store.s_num %></td>
        <td><%= store.s_add %></td>
        <td><%= store.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

